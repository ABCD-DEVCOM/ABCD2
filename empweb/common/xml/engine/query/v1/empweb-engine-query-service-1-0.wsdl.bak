    <wsdl:operation name="getUserStatus">
      <wsdlsoap:operation soapAction=""/>
      <wsdl:input name="getUserStatusRequest">
	      <wsdlsoap:body namespace="http://kalio.net/empweb/engine/query/v1" use="literal"/>
